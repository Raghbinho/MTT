  <object api="DLMS" name="Clock">
    <dlmsObject logicalName="0;0;1;0;0;255" classId="8" classVersion="0"/>
    <description>This IC models the device clock, managing all information related to date and time, including deviations of the local time to a generalized time reference (Greenwich Mean Time, GMT), due to time zones and daylight saving time schemes.</description>
    <attribute id="1" name="logical_name" type="octetstring">
      <dlmsAttribute allowMultipleReferences="false">
        <association name="MNGT" id="1" operations="Get" unicast="true" broadcast="false" multicast="false"/>
        <association name="public" id="16" operations="Get" unicast="true" broadcast="false" multicast="false"/>
        <association name="pre" id="102" operations="-" unicast="true" broadcast="false" multicast="false"/>
      </dlmsAttribute>
      <octetstring name="logical_name" editable="false" edition="asciiText" type-of-codage="Decoded as ASCII" size="6" default="0;0;1;0;0;255;"/>
    </attribute>
	<attribute id="2" name="time" type="octetstring" description="This data contains the meters local date and time, its deviation to GMT and the status">
