        <association name="pre" id="102" operations="-" unicast="true" broadcast="false" multicast="false"/>
      </dlmsAttribute>
<!-- datetime format as specified in EN 62056-62 section 4.4 -->
      <octetstring name="time" editable="true" edition="datetime" type-of-codage="Decoded as Decimal" size="12" default="7;224;8;24;3;3;9;27;255;255;136;128;"/>
    </attribute>
